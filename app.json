{
  "expo": {
    "name": "Transfa Business",
    "slug": "transfa-business",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#523DA2"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.transfa.business"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.transfa.business",
      "googleServicesFile": "./google-services.json",
      "allowBackup": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8bde03c0-c513-4b62-9b53-cb8bf5af8ea7"
      }
    },
    "plugins": ["@react-native-firebase/app", "@notifee/react-native"],
    "scheme": "transfa-business"
  }
}
